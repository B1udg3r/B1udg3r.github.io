group :jekyll_plugins do
  gem "jekyll-remote-theme"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-feed"
end

# Windows performance support (optional but good to have)
gem "wdm", "~> 0.1.1", :install_if => Gem.win_platform?
